{
  "name": "@malopolese/ngx-api-mocks-interceptor",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaloPolese/ngx-api-mocks-interceptor.git"
  },
  "keywords": [
    "angular",
    "interceptor",
    "mocks",
    "mocking"
  ],
  "author": "malopolese",
  "bugs": {
    "url": "https://github.com/MaloPolese/ngx-api-mocks-interceptor/issues"
  },
  "homepage": "https://github.com/MaloPolese/ngx-api-mocks-interceptor#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "build": "ng build",
    "build:lib": "ng build ngx-api-mocks-interceptor && cp README.md dist/ngx-api-mocks-interceptor/",
    "build:demo": "pnpm build:lib && ng build demo --configuration production",
    "watch": "ng build ngx-api-mocks-interceptor --watch --configuration development",
    "test": "ng test ngx-api-mocks-interceptor --watch=false --browsers ChromeHeadless",
    "test:watch": "ng test ngx-api-mocks-interceptor --browsers ChromeHeadless",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "ng lint"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "file-saver": "^2.0.5",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.2.0",
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^10.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.2.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.33.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.40.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
